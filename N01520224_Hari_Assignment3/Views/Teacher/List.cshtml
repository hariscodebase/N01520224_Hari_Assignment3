@model IEnumerable<N01520224_Hari_Assignment3.Models.Teacher>

@{
    ViewBag.Title = "List";
}

<h2>Listing Teachers here</h2>
<div>There are @Model.Count() teachers in the system.</div>

<div>

    @{
        foreach (N01520224_Hari_Assignment3.Models.Teacher Teacher in Model)
        {
            <div>
                <a href="/Teacher/Show/@Teacher.Id">@Teacher.FName @Teacher.LName</a>
            </div>

        }
    }

</div>

